title     Linux Kernel
# Note, the root entry will be hd0,0 in disk image, but hd0 in partition
# it is changed on disk image creation.
root      (hd0)
# FS_LABEL
kernel    /vmlinuz LABEL=cirros-rootfs ro console=tty1 console=ttyS0
initrd    /initrd.img
EOF
